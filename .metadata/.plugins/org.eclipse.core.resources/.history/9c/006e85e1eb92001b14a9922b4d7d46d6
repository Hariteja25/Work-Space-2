package conditionalStatements;

public class revstring {
	public static void main(String[] args) {

		String s = "hello";
		String t = "";
		for (int i = 0; i < s.length(); i++) {

			if(t.contains(s.charAt(i)+"")) {
				
			}
			else {
				t=t+s.charAt(i);
			}
			
		}

		System.out.println(t);
	}

}
