package conditionalStatements;

public class uniqarray {
	public static void main(String[] args) {
		int ar[]= {2,3,4,2,2,3,1,4,2};
		int count=0;
		for(int i=0;i<ar.length;i++) {
			for(int j=0;j<ar.length;j++) {
				if(ar[i]==ar[j]&&i!=j) {
					count++;
					break;
				}
			}
			if(count==0) {
				System.out.println(ar[i]);
			}
		}
	}

}
